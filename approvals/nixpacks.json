{
    "languages": {
      "python": {
        "version": "3.10"
      }
    },
    "phases": {
      "install": {
        "cmds": [
          "pip install -r requirements.txt"
        ]
      },
      "build": {
        "cmds": [
          "python manage.py collectstatic --noinput"
        ]
      },
      "start": {
        "cmds": [
          "gunicorn --worker-tmp-dir /dev/shm myapp.wsgi:application"
        ]
      }
    }
  }
  